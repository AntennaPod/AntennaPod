<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">

        <shape android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="?attr/filter_dialog_clear" />
        </shape>
    </item>

    <!-- x -->
    <item
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp"
        android:top="15dp">
        <rotate
            android:fromDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="135">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="?attr/filter_dialog_clear" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp"
        android:top="15dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="?attr/filter_dialog_clear" />
            </shape>

        </rotate>
    </item>

</layer-list>